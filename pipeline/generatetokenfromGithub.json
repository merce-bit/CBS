{
	"name": "generatetokenfromGithub",
	"properties": {
		"activities": [
			{
				"name": "Web generate token from Github",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://github.com/merce-bit/CBS/blob/main/ELT/endpoints.json",
					"method": "GET",
					"authentication": {
						"type": "Basic",
						"username": "merce-bit",
						"password": {
							"type": "AzureKeyVaultSecret",
							"store": {
								"referenceName": "AzureKeyVault",
								"type": "LinkedServiceReference"
							},
							"secretName": "acesstoken"
						}
					}
				}
			}
		],
		"variables": {
			"Var01": {
				"type": "Array"
			}
		},
		"annotations": []
	}
}