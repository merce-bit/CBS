												"physicalType": "character"
											}
										},
										{
											"source": {
												"name": "management",
												"type": "String",
												"physicalType": "String"
											},
											"sink": {
												"name": "management",
												"type": "String",
												"physicalType": "character"
											}
										},
										{
											"source": {
												"name": "source_data",
